Задача 1 (задайте значения M и N)
Console.Write("Введите число M: ");
int m = Convert.ToInt32(Console.ReadLine());
Console.Write("Введите число N: ");
int n = Convert.ToInt32(Console.ReadLine());
void FindNumbers(int m, int n){

  if (m > n)
     return;

{
     Console.Write($"{m}, ");
 }
 FindNumbers(m + 1, n);

}
FindNumbers(m, n);
